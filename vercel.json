{
    "version": 2,
    "builds": [
        {
            "src": "api/nekopoi.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/nekopoi.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
