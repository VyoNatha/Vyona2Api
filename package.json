{
  "name": "my-api-website",
  "version": "1.0.0",
  "description": "A simple web API built with Node.js and Express",
  "main": "index.js", // Ganti dengan nama file utama Anda
  "scripts": {
    "start": "node index.js", // Perintah untuk menjalankan aplikasi
    "dev": "nodemon index.js" // Perintah untuk pengembangan dengan nodemon
  },
  "engines": {
    "node": "22.x" // Versi Node.js yang diperlukan
  },
  "dependencies": {
    "express": "^4.17.1", // Framework web untuk Node.js
    "cors": "^2.8.5", // Middleware untuk mengizinkan CORS
    "body-parser": "^1.19.0" // Middleware untuk parsing body request
  },
  "devDependencies": {
    "nodemon": "^2.0.7" // Alat untuk otomatisasi restart server saat pengembangan
  },
  "keywords": [
    "api",
    "web",
    "nodejs",
    "express"
  ],
  "author": "Claire", // Ganti dengan nama Anda
